
#NO DIAGONAL
import leonode
from leonode import *
r = Rover()
r.move(3.048)
r.turn(-87)
r.move(1.8288)
r.turn(87)
r.move(1.2192)
r.turn(-87)
r.move(1.2192)

#DIAGONAL
import leonode
from leonode import *
r = Rover()
r.move(3.048)
r.turn(-87)
r.move(1.8288)
r.turn(42)
r.move(1.72420788)

#PYRAMID
import leonode
from leonode import *
r = Rover()
r.move(1.3716)
r.turn(37)
r.move(4.4577)
